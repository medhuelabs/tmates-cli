{
  "name": "tmates-cli",
  "version": "0.1.0",
  "description": "Lightweight Tmates command line client",
  "bin": {
    "tmates": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "tmates",
    "cli"
  ],
  "author": "Tmates",
  "license": "MIT",
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cli-table3": "^0.6.3",
    "dotenv": "^16.4.5",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  }
}
